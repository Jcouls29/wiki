    "version": "0.2.0",
    "configurations": [
      {
        "name": "Launch Chrome",
        "request": "launch",
        "type": "chrome",
        "url": "http://localhost:3000",
        "webRoot": "${workspaceFolder}\\assets"
      },
    {
      "type": "node",
      "request": "attach",
      "name": "Attach (Inspector Protocol)",
      "port": 9229,
      "protocol": "inspector"
    },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "program": "${workspaceRoot}\\server\\index.js"
        },
        {
            "type": "node",
            "request": "attach",
            "name": "Attach to Port",
            "address": "localhost",
            "port": 3000
        },
        {
          "type": "chrome",

        }
    ]
}
